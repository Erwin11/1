{
  "site": {
    "name": "CSViz",
    "description": "WiredViz is visualizatoin of data from Wiredcraft",
    "logo": "../assets/images/logo.png",
    "url": "http://wiredcraft.com"
  },
  "ui": {
    "legend": "false",
    "timeline": "false",
    "about": "false",
    "choropleth": {
      "steps": "5",
      "start": "#ffffff",
      "end": "#000000"
    },
    "menu": {
      "Financial": ["poverty"],
      "Other": ["car_color", "depressed"]
    }
  },
  "data": {
    "globals": "./data/global.json"
  },
  "indicators": {
    "global_growth_numeric": {
      "type": "hidden"
    },
    "poverty": {
      "name": "Average Poverty",
      "id": "avg_poverty",
      "description": "This is the average poverty",
      "aggregate": "avg",
      "precision": "1",
      "type": "number",
      "years": "true"
    },
    "gdp": {
      "name": "GDP",
      "id": "avg_poverty",
      "description": "This is the gdp",
      "aggregate": "avg",
      "precision": "1",
      "type": "number",
      "years": "true"
    },
    "car_color": {
      "name": "Car color",
      "description": "This is the typical car color",
      "type": "select",
      "mapping": {
        "Red": 0,
        "Blue": 30,
        "White": 60,
        "Green": 90
      }
    }
  }
}